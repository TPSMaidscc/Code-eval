# Environment Configuration Example
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Application Environment
ENVIRONMENT=development  # or 'production'
PORT=8000

# Google Service Account Credentials (REQUIRED)
# Production: JSON string in GOOGLE_CREDENTIALS (recommended)
GOOGLE_CREDENTIALS={"type":"service_account","project_id":"your-project","private_key_id":"...","private_key":"-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n","client_email":"your-service@your-project.iam.gserviceaccount.com","client_id":"...","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"https://www.googleapis.com/robot/v1/metadata/x509/...","universe_domain":"googleapis.com"}

# Development: Local file (fallback)
# GOOGLE_SERVICE_ACCOUNT_FILE=cred.json

# Tableau Configuration
TABLEAU_SERVER_URL=https://prod-uk-a.online.tableau.com
TABLEAU_API_VERSION=3.16
TABLEAU_TOKEN_NAME=your_tableau_token_name
TABLEAU_TOKEN_VALUE=your_tableau_token_value
TABLEAU_SITE_CONTENT_URL=your_site_content_url
TABLEAU_WORKBOOK_NAME=8 Department wise tables for chats & calls

# Google Spreadsheet IDs
APPLICANTS_SPREADSHEET_ID=your_applicants_spreadsheet_id
DOCTORS_SPREADSHEET_ID=your_doctors_spreadsheet_id
MV_RESOLVERS_SPREADSHEET_ID=your_mv_resolvers_spreadsheet_id
CC_SALES_SPREADSHEET_ID=your_cc_sales_spreadsheet_id
